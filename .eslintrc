{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": ["airbnb"],
  "rules": {
    "comma-dangle": [2, "always"],
    "no-underscore-dangle": 0,
    "react/prop-types": 1
  }
}
